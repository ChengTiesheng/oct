{
        "Name": "Benchmark-Test-Monitor-docker",
        "Summary": "test with cadvisor tool",
        "Owner": "linzhinan@huawei.com",
        "Description": "Monitor cpu totalusage via cadvisor",
        "Group": "Performance/Benchmark/monitor/cpumonitor/",
        "License": "Apache 2.0",
        "Explains" ""
        "Source": [
                    "./source/Dockerfile",
	       "./source/cpu_overall_usage.go",
                    "./source/deadloop.sh"
                    "./host_deploy.sh"
                    "./docker_deploy.sh"
        ],
        "Requires": [
            {
                
	       "Class":"operationOS", 
	        "Type": "os",
	        "Distribution": "Ubuntu14.04",
	        "Resource": {
	            "CPU": 4,
	            "Memory": "16GB",
	            "Disk": "200G"
	        }
                
            }
            {
                
                "Class": "cpuusage",
                "Type": "container",
                "Version": "dockerV1.0",
                "Files": [
		   "./source/Dockerfile"
		]
                
            }
        ],
        "Deploys": [
            {
                "Object": "hostA",
                "Class": "OperationOS",
                "Cmd":"./host_deploy.sh -f cpu_overall_usage.go",
                "Cmd": "./docker_deploy.sh ",
                "Containers": [
                    {
                        "Object": "cpu",
                        "Class": "cpu_overall_usage"
                    }
                ]
            }         
        ],
        "Run": [
            {
                "Object": "hostA",
                "Class": "OperationOS",
                "Cmd": "cd ./source/",
                "Cmd": "./cpu_overall_usage > cpu_overall_usage.txt"
            }
        ],
        "Collect": [
            {
                "Object": "hostA",
                "Files": ["./source/cpu_overall_usage.txt"]
            }
        ]
    
}

