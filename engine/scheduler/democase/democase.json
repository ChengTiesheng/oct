{"Name": "Network-performance", "License": "GPL",
"Sources": ["./source/Dockerfile", "running.sh"],
"Requires": 
	[
		{"Class": "operationOS",
		 "Type": "os", 
		 "Distribution": "CentOS",
		 "Version": 7
		}, 
		{"Class": "ImageA",
		 "Type": "container",
		 "Distribution": "Docker",
		 "Version": 1,
		 "Files": ["./source/Dockerfile"]
		}
	],
"Deploys": 
	[
		{
                "Object": "hostA",
		"Class": "operationOS",
		"Files": ["running.sh"],
		"Cmd": "touch /tmp/dliang",
		"Containers": [
				{"Object": "DockerA",
				 "Class": "ImageA",
				 "Cmd": "touch /tmp/dliang_d1",
				 "Files": ["./source/Dockerfile"]
				}
			]
          	}
	]
}


