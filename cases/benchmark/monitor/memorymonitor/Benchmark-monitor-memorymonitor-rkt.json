{
        "Name": "Benchmark-Test-Monitor-docker",
        "Summary": "test with cadvisor tool",
        "Owner": "liugenping@huawei.com",
        "Description": "Monitor memory totalusage via cadvisor",
        "Group": "Performance/Benchmark/monitor/memorymonitor/",
        "License": "Apache 2.0",
        "Explains": "",
        "Source": [
	      "./source/memorymonitor.go",
                    "./host_deploy.sh",
                    "./rkt_deploy.sh"
        ],
        "Requires": [
            {
             
	       "Class":"OperationOS", 
	        "Type": "os",
	        "Distribution": "Ubuntu",
		"Version": "14.04",
	        "Resource": {
	        "CPU": 4,
	        "Memory": "16GB",
	        "Disk": "200G"
	        }   
            }
        ],
        "Deploys": [
            {
                "Object": "hostA",
                "Class": "OperationOS",
                "Cmd":"./host_deploy.sh -f memorymonitor.go; ./rkt_deploy.sh"
            }         
        ],
        "Run": [
            {
                "Object": "hostA",
                "Class": "OperationOS",
                "Cmd": "cd ./source/ ; ./memorymonitor rkt"
            }
        ],
        "Collects": [
            {
                "Object": "hostA",
                "Files": ["./source/rkt_memory.json"]
            }
        ]
    
}

