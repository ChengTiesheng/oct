{
  "Name": "Specstest-root_readonly_false-runc",
  "Summary": "test based opencontainers/specs",
  "Owner": "linzhinan@huawei.com",
  "Description": "Test runc when spec root:readonly == false",
  "Group": "Specstest/root_readonly_false/",
  "License": "Apache 2.0",
  "Explains": "Test runc when spec root:readonly == false",
  "Requires": [
    {
      "Class": "OperationOS",
      "Type": "os",
      "Distribution": "ubuntu",
      "Version": "14.04",
      "Resource": {
        "CPU": 1,
        "Memory": "1GB",
        "Disk": "2G"
      }
    },
    {
      "Class": "specstest",
      "Type": "container",
      "Distribution": "runc",
      "Version": "0.2",
      "Files": [
        "./source/config.json"
      ]
    }
  ],
  "Deploys": [
    {
      "Object": "hostA",
      "Class": "OperationOS",
      "Files": [
        "./source/root_readonly_false.go",
        "./source/root_readonly_false_host.go"
      ],
      "Cmd": "go build root_readonly_false_host.go; ./root_readonly_false_host",
      "Containers": [
        {
          "Object": "specs",
          "Class": "specstest"
        }
      ]
    }
  ],
  "Run": [
    {
      "Object": "hostA",
      "Class": "OperationOS",
      "Cmd": "cd ./../../source/ ; runc"
    }
  ],
  "Collects": [
    {
      "Object": "hostA",
      "Files": [
        "/tmp/testtool/readonly_false_out.txt"
      ]
    }
  ]
}
