{
  "Name": "Network-netns-create",
  "Summary": "Test network namespace create function",
  "Owner": "chengtiesheng@huawei.com",
  "Description": "Test network namespace create function",
  "Group": "network/netns-create/",
  "License": "Apache 2.0",
  "Explains": "",
  "Requires": [
    {
      "Class": "OperationOS",
      "Type": "os",
      "Distribution": "ubuntu",
      "Version": "14.04",
      "Resource": {
        "CPU": 1,
        "Memory": "1GB",
        "Disk": "2G"
      }
    }
  ],
  "Deploys": [
    {
      "Object": "hostA",
      "Class": "OperationOS",
      "Files": [
        "./source/netns_create.go",
      ],
      "Cmd": "go build netns_create.go; ./netns_create",
    }
  ],
  "Run": [
    {
      "Object": "hostA",
      "Class": "OperationOS",
      "Cmd": "cd ./../../source/ ; ./netns_create"
    }
  ],
  "Collects": [
    {
      "Object": "hostA",
      "Files": [
        "netns-create-res.json"
      ]
    }
  ]
}
